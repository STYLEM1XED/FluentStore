<Project Sdk="Microsoft.NET.Sdk">

    <ItemGroup>
        <PackageReference Include="Octokit" Version="13.0.1" />
        <ProjectReference Include="..\..\FluentStore.SDK\FluentStore.SDK.csproj" />
    </ItemGroup>

    <PropertyGroup>
        <Title>GitHub</Title>
        <Version>$(FluentStoreMajorMinorVersion).0.2</Version>
        <PackageVersion>$(Version)-alpha</PackageVersion>
        <Description>Provides support for viewing GitHub repositories as packages. Downloading and installing may be supported for some repositories. Searching for is not supported.</Description>
    </PropertyGroup>

</Project>
